{
  "name": "heyui",
  "version": "1.28.0",
  "description": "UI Toolkit for Web, Vue2.0.",
  "scripts": {
    "buildlang": "vite build --config ./scripts/vite.lang.js",
    "buildcom": "vite build --config ./scripts/vite.components.js",
    "cp": "rm -rf ../heyui-dist && mkdir ../heyui-dist && cp -R ./dist ../heyui-dist && cp -R ./lib ../heyui-dist && cp -R ./lang ../heyui-dist && cp -R ./themes ../heyui-dist && cp  ./package.json ../heyui-dist",
    "build": "vite build",
    "create": "node ./scripts/create.js",
    "varless": "lessto --json themes/var.less themes/var.json && lessto --js themes/var.less themes/var.js",
    "p": "sh ./scripts/publish.sh",
    "pa": "sh ./scripts/publish.sh -alpha",
    "pb": "sh ./scripts/publish.sh -beta",
    "release": "standard-version"
  },
  "main": "./dist/heyui.js",
  "module": "./dist/heyui.esm.js",
  "dependencies": {
    "manba": "^1.3.3",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.3",
    "art-template": "^4.13.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "commander": "^2.19.0",
    "copy-webpack-plugin": "^6.4.1",
    "eslint": "^6.7.2",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0-0",
    "headr": "0.0.4",
    "hey-log": "^0.0.8",
    "hey-utils": "^1.0.2",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "less-to-json": "^1.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "standard-version": "^9.3.0",
    "vite": "^2.6.4"
  },
  "files": [
    "dist",
    "lang",
    "themes",
    "lib",
    "README.md",
    "simple.html"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "copyright": "Copyright Â© 2017-present Lan",
  "homepage": "http://www.heyui.top",
  "keywords": [
    "heyui",
    "vue",
    "ui",
    "framework"
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heyui/heyui.git"
  },
  "standard-version": {
    "commitAll": true,
    "message": "chore(release): %s",
    "skip": {
      "tag": true
    },
    "scripts": {
      "precommit": "sh ./scripts/headr.sh"
    }
  }
}
